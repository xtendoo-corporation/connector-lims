<odoo>

    <record id="analysis_numerical_resutl_form" model="ir.ui.view">
        <field name="name">analysis.numerical.result.form</field>
        <field name="model">lims.analysis.numerical.result</field>
        <field name="arch" type="xml">
            <form>
                <label for="component_ids" />
                <field name="component_ids" />
                <br />
            </form>
        </field>
    </record>

    <record id="analysis_numerical_result_tree" model="ir.ui.view">
        <field name="name">analysis.components.limit.result.tree</field>
        <field name="model">lims.analysis.numerical.result</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="analysis_ids" readonly="1" />
                <field name="component_ids" readonly="1" />
                <field name="value" />
                <field name="component_uom" />
                <field name="limit_value" />
                <field name="between_limit_value" />
                <field name="is_null" />
                <field name="corrected_value" />
                <field name="loq" />
                <field name="corrected_loq" />
                <field name="dil_fact" />
                <field name="reason" />
                <field name="comment" />
            </tree>
        </field>
    </record>

</odoo>
