<?xml version="1.0" ?>
<odoo>
    <record id="analysis_lims.product_template_form_view" model="ir.ui.view">
        <field name="name">product.template.lims.form</field>
        <field name="model">product.template</field>
        <field
            name="inherit_id"
            ref="product_attribute_lims.product_template_form_view"
        />
        <field name="arch" type="xml">
            <page name="product_lims" position="inside">
                <field name="have_analysis" />
                <label for="have_analysis" />
                <label for="quality_checks_ids" />
                <field
                    name="quality_checks_ids"
                    widget="many2many_tags"
                    groups="base.group_no_one"
                />
            </page>
        </field>
    </record>
</odoo>
