<?xml version="1.0" ?>
<odoo>
    <record id="analysis_lims_form" model="ir.ui.view">
        <field name="name">analysis.lims.form</field>
        <field name="model">analysis.lims</field>
        <field name="arch" type="xml">
            <form>
                <field name="name" />

            </form>
        </field>
    </record>

     <record id="analysis_lims_tree" model="ir.ui.view">
        <field name="name">analysis.lims.tree</field>
        <field name="model">analysis.lims</field>
        <field name="arch" type="xml">
            <form>
                <tree>
                    <field name="name" />
                </tree>
            </form>
        </field>
    </record>


    <record id="quality_checks_action" model="ir.actions.act_window">
        <field name="name">Quality Checks</field>
        <field name="res_model">anlysis.lims</field>
        <field name="view_mode">tree</field>
        <field name="target">current</field>
         <field name="domain">[('quality_checks_ids', '!=', '1')]</field>
    </record>
</odoo>
