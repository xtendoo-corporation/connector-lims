<?xml version="1.0" ?>
<odoo>
    <record id="analysis_line_lims_form" model="ir.ui.view">
        <field name="name">analysis.line.lims.form</field>
        <field name="model">analysis.line.lims</field>
        <field name="arch" type="xml">
            <form>
                <field name="name" />
                <label for="priority" />
                <field name="priority" />
                <group name="Sample Information" string="Sample Information">
                    <field name="product_id" />
                    <field name="description" />
                    <field name="lot_id" />
                    <!--<field name="regulation"></field>-->
                </group>
                <group name="General Information" string="General Information">
                    <field name="laboratory_id" />
                    <field name="customer_id" />
                    <field name="customer_contact_id" />
                    <field name="reason" />
                    <field name="reference" />
                   <!--<field name="active"></field>-->
                    <!--<field name="tag_ids"></field>-->
                    <field name="date_plan" />
                    <field name="date_due" />
                    <field name="date_sample" />
                    <field name="date_sample_begin" />
                    <field name="sampler" />
                </group>

            </form>
        </field>
    </record>

    <record id="analysis_line_lims_tree" model="ir.ui.view">
        <field name="name">analysis.line.lims.tree</field>
        <field name="model">analysis.line.lims</field>
        <field name="arch" type="xml">
            <form>
                <tree>
                    <field name="name" />
                </tree>
            </form>
        </field>
    </record>
</odoo>
